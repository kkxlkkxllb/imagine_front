@import './mixin'

@font-face
   	font-family Dancing Script
   	font-style normal
   	src url('/font/Dancing Script.woff')
@font-face
   	font-family Lobster
   	font-style normal
   	src url('/font/Lobster.woff')
@font-face
  	font-family 'Pacifico'
  	font-style normal
  	src url('/font/Pacifico.woff')

.dancing
	font-family Dancing Script
.lobster
	font-family Lobster
.pacifico
	font-family Pacifico
html,body
	margin 0
	padding 0
	width 100%
	height 100%
	overflow hidden

html
	hbg-gradient(#1e5799,#2989d8)

p
	text-align center
	font-size 16px

.hiddenfile
	width: 0px
	height: 0px
	overflow: hidden
	position: absolute
	bottom: 0
	left: 0
